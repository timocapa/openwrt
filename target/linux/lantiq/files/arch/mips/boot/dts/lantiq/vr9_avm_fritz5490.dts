#include "vr9_avm_fritzxx90.dtsi"

/ {
	compatible = "avm,fritz5490", "lantiq,xway", "lantiq,vr9";
	model = "AVM FRITZ!Box 5490";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x20000000>;
	};
};

&eth0 {
	mtd-mac-address = <&urlader 0xcc>;
	mtd-mac-address-increment = <1>;
};

&gswip_mdio {
	phy0: ethernet-phy@0 {
		reg = <0x00>;
		reset-gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
		qca,ar8327-initvals = <
			0x07C 0x000000FB	/* PORT0 STATUS_REG */
			0x09C 0x00000000	/* PORT0 HDRCTRL_REG */
			0x0A4 0x00000000	/* PORT2 HDRCTRL_REG */
			0x0A8 0x00000000	/* PORT3 HDRCTRL_REG */
			0x620 0x01400400	/* GLOFW_CTRL0_REG */
			0x624 0x01010101	/* GLOFW_CTRL1_REG */
			0x660 0x000C023E	/* PORT0_LOOKUP_CTRL_REG */
			0x678 0x000C023E	/* PORT0_LOOKUP_CTRL_REG */
			0x684 0x000C023E	/* PORT0_LOOKUP_CTRL_REG */
			0x9A0 0x2A666666	/* PORT0_HOL_CTRL0 */
			0x9A4 0x000000C6	/* PORT0_HOL_CTRL1 */
			0x980 0x2A666666	/* PORT2_HOL_CTRL0 */
			0x984 0x000000C6	/* PORT2_HOL_CTRL1 */
			0x988 0x2A666666	/* PORT3_HOL_CTRL0 */
			0x98C 0x000000C6	/* PORT3_HOL_CTRL1 */
		>;
	};

	phy1: ethernet-phy@1 {
		reg = <0x01>;
	};

	phy2: ethernet-phy@2 {
		reg = <0x02>;
	};

	phy5: ethernet-phy@5 {
		reg = <0x05>;
	};

	phy6: ethernet-phy@6 {
		reg = <0x06>;
		reset-gpios = <&gpio 32 GPIO_ACTIVE_LOW>;
	};

	phy9: ethernet-phy@9 {
		reg = <0x09>;
	};

	phy11: ethernet-phy@11 {
	};

	phy13: ethernet-phy@13 {
	};
};

&gswip_ports {
	port@0 {
		reg = <0>;
		label = "wan";
		phy-mode = "rgmii-rxid";
		phy-handle = <&phy6>;
	};

	port@1 {
		reg = <1>;
		label = "lan3";
		phy-mode = "rgmii";

		fixed-link {
			speed = <1000>;
			full-duplex;
		};
	};

	port@2 {
		reg = <2>;
		label = "lan2";
		phy-mode = "internal";
		phy-handle = <&phy9>;
	};

	port@4 {
		reg = <4>;
		label = "lan1";
		phy-mode = "internal";
		phy-handle = <&phy5>;
	};
};

&gpio {
	usb-vbus {
		gpio-hog;
		line-name = "usb-vbus";
		gpios = <14 GPIO_ACTIVE_HIGH>;
		output-high;
	};

	wasp-wakeup {
		gpio-hog;
		line-name = "wasp-wakeup";
		gpios = <5 GPIO_ACTIVE_HIGH>;
		output-high;
	};
};

&pci0 {
	status = "okay";
};
